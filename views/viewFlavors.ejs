        <div id="page-wrapper">
            
            <div class="row">
                <div class="col-lg-12">
                    <h3>View Flavors</h3>
                    
                      <div class="panel panel-default">

                          <div class="panel-heading">
                                Flavors
                            </div>
                          
                        <div class="panel-body">
                            
                            <div class="table-responsive table-bordered">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>Flavor Name</th>
                                                                                     
                                            <th>RAM</th>  
                                            <th>VCPUS</th>
                                    </thead>
                                    <tbody>
                                        
                                             <% flavors.forEach(function(flavor) { %>
                                            <tr>
                                            <td><%= flavor.name %></td>
                                           
                                            <td><%= flavor.ram %></td>
                                            <td><%= flavor.vcpus %></td>
                                            </tr>
                                            <% }); %>
                                            

                                    </tr>
                                    </tbody>
                                </table>
                            </div>          <!-- /.table-responsive -->

                         </div>             <!-- /.panel-body -->
                        
                      </div>                <!-- /.panel -->

                          <!-- /.panel-body --> 

